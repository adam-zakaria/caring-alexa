{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "lambda:CreateFunction",
                "lambda:GetFunction",
                "lambda:UpdateFunctionCode",
                "lambda:UpdateFunctionConfiguration",
                "lambda:AddPermission",
                "lambda:ListVersionsByFunction",
                "lambda:GetPolicy"
            ],
            "Resource": "arn:aws:lambda:us-east-1:462099094434:function:mycare-skill-lambda"
        },
        {
            "Effect": "Allow",
            "Action": "iam:PassRole",
            "Resource": "arn:aws:iam::462099094434:role/alexa-skill-lambda-role"
        }
    ]
}
